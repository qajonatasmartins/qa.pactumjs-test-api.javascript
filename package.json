{
  "name": "data-management",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "data_management_test": "mocha ./data-management/tests/**/**.test.js --reporter spec --timeout 5000",
    "test_e2e": "mocha ./e2e/tests/**/**.test.js --reporter spec --timeout 5000",
    "test_mock": "mocha ./mock/tests/**/**.test.js --reporter spec --timeout 5000",
    "clean": "rimraf allure-report & rimraf allure-results",
    "test_allure": "npm run clean && mocha ./reporter-allure/tests/**/**.test.js --reporter mocha-allure-reporter && npm run allure-report-gen",
    "allure-report-gen": "npx allure generate ./allure-results && allure open",
    "test_mochawesome": "mocha --spec ./reporter-mochawesome/tests/**.js --reporter mochawesome --require mochawesome/register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mocha": "^10.2.0",
    "pactum": "^3.4.1",
    "allure-commandline": "^2.23.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome": "^7.1.3",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2"
  }
}